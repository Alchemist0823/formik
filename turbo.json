{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
    "build": {
      // ^build means build must be run in dependencies
      // before it can be run in this workspace
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "prepublish": {
      // ^build means build must be run in dependencies
      // before it can be run in this workspace
      "dependsOn": ["^prepublish"],
      "outputs": ["dist/**"]
    },
    "test": {},
    "lint": {}
  }
}
